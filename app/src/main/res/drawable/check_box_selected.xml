<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:padding="3dp">
    <item>
        <shape
            android:shape="rectangle"
            android:useLevel="false">
            <stroke
                android:width="2dp"
                android:color="@color/grey" />
            <size
                android:width="25dp"
                android:height="25dp" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape
            android:shape="rectangle"
            android:useLevel="false">

            <solid android:color="@color/greyDark" />
        </shape>
    </item>
</layer-list>